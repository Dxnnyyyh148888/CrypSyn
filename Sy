                                                    game:GetService("StarterGui"):SetCore("DevConsoleVisible", true)
                                                    end)
                                                    local v = Instance.new("TextButton")
                                                    v.Parent = mics
                                                    v.Size = UDim2.new(0.3,0,0.15)
